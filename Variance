 sim_est |>\n","  filter(year == 2020) |>\n","  mutate(st = str_sub(cd, 1, 2)) |>\n","  left_join(ccesMRPprep::states_key, by = \"st\") |>\n","  rename(p_mrp = p_mrp_twway)\n","\n","# model 1: base model race + CD + interaction\n","fit_lm0 <- aov(\n","  p_mrp ~ race + cd + (race:cd),\n","  filter(sim_est_20, iter %% 10 == 1)\n",")\n","\n","# model 2: race + all geography levels + interactions\n","fit_lm <- aov(\n","  p_mrp ~ race + region + st + cd + (race:region) + (race:st) + (race:cd),\n","  filter(sim_est_20, iter %% 10 == 1)\n",")\n","\n","# Formula for race models\n","ff_g <- p_mrp ~ region + st + cd\n","\n","# model 3-5 race models --> geography only, by race\n","fitW_lm <- aov(ff_g, filter(sim_est_20, race == \"White\", iter %% 10 == 1))\n","fitB_lm <- aov(ff_g, filter(sim_est_20, race == \"Black\", iter %% 10 == 1))\n","fitH_lm <- aov(ff_g, filter(sim_est_20, race == \"Hispanic\", iter %% 10 == 1))\n"]},{"cell_type":"code","source":["\n","fmt_var_lm <- function(aov, accr = 0.01) {\n","  df_pct <- summary(aov)[[1]] |>\n","    as_tibble(rownames = \"grp\") |>\n","    janitor::clean_names() |>\n","    mutate(\n","      frac = scales::percent(sum_sq / sum(sum_sq), accuracy = accr, scale = 1, suffix = \"\")\n","    ) |>\n","    mutate(grp = str_squish(grp))\n","\n","  # Calculate total variance\n","  df_var <- df_pct |>\n","    summarize(df = sum(df) + 1, sum_sq = sum(sum_sq)) |>\n","    mutate(grp = \"Total Variance\", qty = formatC(sum_sq / df, digits = 2))\n","\n","  # sample size\n","  df_n <- tibble(\n","    grp = \"Samples\",\n","    qty = formatC(nobs(aov), big.mark = \",\")\n","  )\n","\n","  # Combine all rows\n","  bind_rows(df_pct, df_var, df_n)\n","}\n","\n","# ==========================================================="],"metadata":{"id":"RrEcD_Wj0HvZ","executionInfo":{"status":"ok","timestamp":1765830148245,"user_tz":300,"elapsed":35,"user":{"displayName":"Amanda Huang","userId":"13335420900045939996"}}},"execution_count":23,"outputs":[]},{"cell_type":"code","source":["\n","fraclm_long <- list(\n","  base = fmt_var_lm(fit_lm0),\n","  overall = fmt_var_lm(fit_lm),\n","  raceW = fmt_var_lm(fitW_lm),\n","  raceB = fmt_var_lm(fitB_lm),\n","  raceH = fmt_var_lm(fitH_lm)\n",") |>\n","  map_dfr(.id = \"model\", .f = ~ select(.x, grp, qty, frac))\n","\n","fraclm_wide <- fraclm_long |>\n","  mutate(\n","    frac = str_remove(frac, \"^0\"),\n","    frac = str_squish(coalesce(frac, qty))\n","  ) |>\n","\n","  # pivot wider\n","  pivot_wider(id_cols = grp,\n","            values_from = frac,\n","            names_from = model) |>\n","  # Reorder rows logically\n","  mutate(\n","    grp = fct_relevel(\n","      grp,\n","      \"race\", \"cd\", \"st\", \"region\",\n","      \"race:cd\", \"race:st\", \"race:region\",\n","      \"Residuals\",\n","      \"Total Variance\", \"Samples\"\n","    )\n","  ) |>\n","  arrange(grp) |>\n","  # Clean up labels for display\n","  mutate(\n","    grp = str_to_title(grp),\n","    grp = recode(\n","      grp,\n","      St = \"State\",\n","      Cd = \"Congressional District\",\n","      Race = \"Racial Group\",\n","      Residuals = \"Residuals\",\n","      `Race:region` = \"Race × Region\",\n","      `Race:st` = \"Race × State\",\n","      `Race:cd` = \"Race × Congressional District\"\n","    )\n","  )"],"metadata":{"id":"-F0SW7E3CdVb","executionInfo":{"status":"ok","timestamp":1765830150815,"user_tz":300,"elapsed":293,"user":{"displayName":"Amanda Huang","userId":"13335420900045939996"}}},"execution_count":24,"outputs":[]},{"cell_type":"code","source":["\n","install.packages(\"gt\")\n","library(gt)\n","\n","fraclm_gt <- fraclm_wide |>\n","  gt() |>\n","\n","  tab_header(\n","    title = \"Variance Decomposition of Trump Vote (2020)\",\n","    subtitle = \"Fraction of variation explained by different components\"\n","  ) |>\n","  cols_label(\n","    grp = \"\",\n","    base = \"(1)\",\n","    overall = \"(2)\",\n","    raceW = \"(3)\",\n","    raceB = \"(4)\",\n","    raceH = \"(5)\"\n"," 